{
  "name": "frontendweekend",
  "version": "1.0.0",
  "description": "![alt text](https://s3.amazonaws.com/creativetim_bucket/products/27/original/opt_alp_thumbnail.jpg \"Awesome Landing Page\")",
  "main": "index.js",
  "scripts": {
    "start": "live-server public",
    "deploy": "surge ./public https://frontendweekend.ml",
    "deploy-stage": "surge ./public https://stage.frontendweekend.ml",
    "test": "jest",
    "codacy-coverage": "jest --coverage && cat ./coverage/lcov.info | codacy-coverage",
    "csslint": "csslint src",
    "stylelint": "stylelint src/**/*.css",
    "stylelint-fix": "stylelint --fix src/**/*.css",
    "eslint": "eslint src",
    "eslint-junit": "eslint --format junit -o reports/junit/js-lint-results.xml src",
    "eslint-all": "eslint .",
    "eslint-all-junit": "eslint --format junit -o reports/junit/js-lint-results.xml .",
    "eslint-fix": "eslint --fix src",
    "eslint-fix-all": "eslint --fix .",
    "e2e-test": "start-server-and-test start http://localhost:8080 cy:run",
    "test-lh": "JEST_JUNIT_OUTPUT=./results/junit.xml jest --runInBand --ci --reporters=default --reporters=jest-junit lighthouse.test.js",
    "cy:run": "cypress run --record --key 7368ff41-1746-43fd-acc5-f6aa54b67f97 --reporter junit --reporter-options \"mochaFile=results/my-test-output.xml\"",
    "cypress-open": "cypress open",
    "gatsby-develop": "gatsby develop",
    "gatsby-develop-https": "gatsby develop --https",
    "gatsby-build": "gatsby build",
    "gatsby-serve": "gatsby serve",
    "sc-import": "node ./sc-import.js",
    "sc-file-import": "node ./sc-file-import.js",
    "sc-comment-import": "node ./sc-comment-import.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxdie/frontendweekend.git"
  },
  "keywords": [],
  "author": "Artem Tsatsin <nuxdie@gmail.com> (https://github.com/nuxdie)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nuxdie/frontendweekend/issues"
  },
  "homepage": "https://github.com/nuxdie/frontendweekend#readme",
  "dependencies": {
    "gatsby": "2.0.67",
    "react": "16.6.3",
    "react-dom": "16.6.3"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "chrome-launcher": "0.10.5",
    "codacy-coverage": "3.3.0",
    "csslint": "1.0.5",
    "cypress": "3.1.4",
    "davefeedread": "0.4.14",
    "eslint": "5.10.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-node": "8.0.0",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-react": "7.11.1",
    "eslint-plugin-standard": "4.0.0",
    "gatsby-cli": "2.4.6",
    "gatsby-plugin-google-tagmanager": "2.0.7",
    "gatsby-plugin-html-attributes": "1.0.5",
    "gatsby-plugin-i18next": "1.1.6",
    "gatsby-plugin-manifest": "2.0.12",
    "gatsby-plugin-netlify-cms": "3.0.9",
    "gatsby-plugin-offline": "2.0.19",
    "gatsby-plugin-react-helmet": "3.0.4",
    "gatsby-plugin-sitemap": "2.0.3",
    "gatsby-plugin-typography": "2.2.2",
    "gatsby-remark-component": "1.1.3",
    "gatsby-source-filesystem": "2.0.12",
    "gatsby-transformer-remark": "2.1.15",
    "gatsby-transformer-yaml": "2.1.6",
    "jest": "23.6.0",
    "jest-css-modules": "1.1.0",
    "jest-junit": "5.2.0",
    "lighthouse": "4.0.0-alpha.2-3.2.1",
    "live-server": "1.2.1",
    "moment": "2.23.0",
    "netlify-cms": "2.3.2",
    "node-fetch": "2.3.0",
    "react-helmet": "5.2.0",
    "react-icons": "3.2.2",
    "react-share": "2.4.0",
    "react-test-renderer": "16.7.0",
    "react-typography": "0.16.18",
    "rehype-react": "3.1.0",
    "simple-icons": "1.9.17",
    "start-server-and-test": "1.7.11",
    "stylelint": "9.9.0",
    "stylelint-config-css-modules": "1.3.0",
    "stylelint-config-recommended": "2.1.0",
    "stylelint-config-standard": "18.2.0",
    "stylelint-order": "2.0.0",
    "surge": "0.20.1",
    "typography": "0.16.18",
    "unique-random-array": "1.0.1",
    "wget": "0.0.1"
  }
}
